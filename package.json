{
    "name": "snapswot",
    "version": "1.17.1",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "release": "dotenv -e .env.local release-it"
    },
    "dependencies": {
        "@prismicio/client": "^7.2.0",
        "@sentry/nextjs": "^7.70.0",
        "@supabase/supabase-js": "^2.36.0",
        "axios": "^1.5.0",
        "classnames": "^2.3.2",
        "formik": "^2.4.5",
        "image-size": "^1.0.2",
        "lodash.debounce": "^4.0.8",
        "luxon": "^3.4.3",
        "next": "^12.3.4",
        "next-seo": "^5.15.0",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-countdown": "^2.3.5",
        "react-dom": "^18.2.0",
        "react-modal": "^3.16.1"
    },
    "devDependencies": {
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.49.0",
        "eslint-config-next": "^12.3.4",
        "eslint-config-prettier": "^8.10.0",
        "sass": "^1.68.0"
    },
    "release-it": {
        "git": {
            "changelog": "npx auto-changelog --stdout --commit-limit false --unreleased --template https://raw.githubusercontent.com/release-it/release-it/master/templates/changelog-compact.hbs",
            "commitMessage": "Releasing v${version}",
            "tagName": "v${version}"
        },
        "github": {
            "release": true,
            "releaseName": "v${version}",
            "tokenRef": "GITHUB_TOKEN"
        },
        "hooks": {
            "after:bump": "npx auto-changelog -p --template changelog-template.hbs --ignore-commit-pattern \"^(Releasing v.*)\""
        },
        "npm": {
            "publish": false
        }
    }
}
